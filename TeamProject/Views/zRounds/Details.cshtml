@model TeamProject.zRound

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>zRound</legend>

    <div class="display-label">StartDate</div>
    <div class="display-field">@String.Format("{0:g}", Model.StartDate)</div>

    <div class="display-label">EndDate</div>
    <div class="display-field">@String.Format("{0:g}", Model.EndDate)</div>

    <div class="display-label">zRequest</div>
    <div class="display-field">@(Model.zRequest == null ? "None" : Model.zRequest.Count.ToString())</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.RoundNo }) |
    @Html.ActionLink("Back to List", "Index")
</p>


